{
  "name": "ShareNotes AI 增强解析",
  "description": "输入内容链接，启用 AI 增强（摘要/标题优化）后创建 flomo 笔记。",
  "steps": [
    {"action": "AskForInput", "prompt": "请输入内容链接"},
    {"action": "GetContentsOfURL", "method": "POST", "url": "https://your-domain.vercel.app/api/parse", "headers": {"Content-Type": "application/json"}, "body": {"url": "{{Input}}", "output_format": "flomo", "ai_enhance": true, "ai_options": {"enable_summary": true, "enable_title_optimization": true}}},
    {"action": "GetDictionaryValue", "key": "ios_url"},
    {"action": "OpenURL"}
  ]
}
